{
    "name": "maba/twig-template-modification-bundle",
    "description": "Bundle to Mass-Modify Twig Templates",
    "keywords": ["twig", "mass-edit", "mass-replace", "find-replace", "mass-modify", "modification", "templates"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marius Balčytis",
            "email": "marius.balcytis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "twig/twig": "^1.20",
        "symfony/symfony": "~2.8.27",
        "symfony/assetic-bundle": "2.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Maba\\Bundle\\TwigTemplateModificationBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maba\\Tests\\": "test"
        }
    }
}
